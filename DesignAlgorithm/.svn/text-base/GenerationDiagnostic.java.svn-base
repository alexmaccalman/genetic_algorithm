
package DesignAlgorithm;

/**
 *
 * @author maccalman
 */
public class GenerationDiagnostic {

    private int generationNumber;
    private double minFitness;
    private double maxFitness;
    private double fitnessRange;
    private double fitnessMean;
    private double fitnessTotal;
    private int numberColumns;
    private double time;
    private double minCorrelation;


    public GenerationDiagnostic(int generationNumber, double minFitness,
            double maxFitness, double fitnessRange, double fitnessMean, 
            double fitnessTotal, int numberColumns, double time, double minCorrelation){
        this.generationNumber = generationNumber;
        this.minFitness = minFitness;
        this.maxFitness = maxFitness;
        this.fitnessRange = fitnessRange;
        this.fitnessMean = fitnessMean;
        this.fitnessTotal = fitnessTotal;
        this.numberColumns = numberColumns;
        this.time = time;
        this.minCorrelation = minCorrelation;
    }

    /**
     * @return the minFitness
     */
    public double getMinFitness() {
        return minFitness;
    }

    /**
     * @param minFitness the minFitness to set
     */
    public void setMinFitness(double minFitness) {
        this.minFitness = minFitness;
    }

    /**
     * @return the maxFitness
     */
    public double getMaxFitness() {
        return maxFitness;
    }

    /**
     * @param maxFitness the maxFitness to set
     */
    public void setMaxFitness(double maxFitness) {
        this.maxFitness = maxFitness;
    }

    /**
     * @return the fitnessRange
     */
    public double getFitnessRange() {
        return fitnessRange;
    }

    /**
     * @param fitnessRange the fitnessRange to set
     */
    public void setFitnessRange(double fitnessRange) {
        this.fitnessRange = fitnessRange;
    }

    /**
     * @return the fitnessMean
     */
    public double getFitnessMean() {
        return fitnessMean;
    }

    /**
     * @param fitnessMean the fitnessMean to set
     */
    public void setFitnessMean(double fitnessMean) {
        this.fitnessMean = fitnessMean;
    }

    /**
     * @return the fitnessTotal
     */
    public double getFitnessTotal() {
        return fitnessTotal;
    }

    /**
     * @param fitnessTotal the fitnessTotal to set
     */
    public void setFitnessTotal(double fitnessTotal) {
        this.fitnessTotal = fitnessTotal;
    }

    /**
     * @return the generationNumber
     */
    public int getGenerationNumber() {
        return generationNumber;
    }

    /**
     * @param generationNumber the generationNumber to set
     */
    public void setGenerationNumber(int generationNumber) {
        this.generationNumber = generationNumber;
    }

    /**
     * @return the numberColumns
     */
    public int getNumberColumns() {
        return numberColumns;
    }

    /**
     * @param numberColumns the numberColumns to set
     */
    public void setNumberColumns(int numberColumns) {
        this.numberColumns = numberColumns;
    }

    /**
     * @return the time
     */
    public double getTime() {
        return time;
    }

    /**
     * @param time the time to set
     */
    public void setTime(double time) {
        this.time = time;
    }

    /**
     * @return the minCorrelation
     */
    public double getMinCorrelation() {
        return minCorrelation;
    }

    /**
     * @param minCorrelation the minCorrelation to set
     */
    public void setMinCorrelation(double minCorrelation) {
        this.minCorrelation = minCorrelation;
    }
    
}
